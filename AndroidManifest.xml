<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ieeeexpenseapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.ieeeexpenseapp.Login"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.ieeeexpenseapp.Register"
            android:label="@string/title_activity_register" >
        </activity>
        <activity
            android:name="com.example.ieeeexpenseapp.EnterExpense"
            android:label="@string/title_activity_enter_expense" >
        </activity>
        <activity
            android:name="com.example.ieeeexpenseapp.ViewExpense"
            android:label="@string/title_activity_view_expense" >
        </activity>
    </application>

</manifest>
